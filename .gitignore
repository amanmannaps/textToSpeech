!.elasticbeanstalk/*.global.yml

# sitemap file
public/sitemap.xml.gz
public/sitemap.xml

# erd(Entity-Relationship Diagrams) file
erd.pdf

# public assets
public/assets

# ignore node
node_modules/

# ignore local scripts
scripts/

.idea/
vendor/bundle/
/public/packs
/public/packs-test
/node_modules
yarn-debug.log*
.yarn-integrity

